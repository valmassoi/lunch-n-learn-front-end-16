  "dependencies": {
    "axios": "^0.11.1",
    "express": "^4.14.0",
    "history": "^1.17.0",
    "lodash": "^4.13.1",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.4.1",
    "react-marked-markdown": "^1.4.2",
    "react-router": "^3.0.0"
